[user]
  email = alexandre.bossard@tanker.io
  name = Alexandre Bossard
[color]
  branch = auto
  diff = auto
  grep = auto
  interactive = auto
  pager = true
  status = auto
  showbranch = auto
  ui = auto
[alias]
  st = status
  co = checkout
  cm = commit
  fixup = commit --fixup
  rb = rebase
  dif = diff
  l = log
  sdif = diff --staged
  sdiff = diff --staged
  lp = log -p
  ls = log --oneline
  lss = log --oneline --stat
  lsr = log --oneline --reverse
  lg = log --graph --abbrev-commit --decorate --all
  wip = commit -m '[skip ci] wip' --allow-empty
[push]
  default = simple
[core]
  editor = vim
  excludesfile = /home/alexandre/.gitignore
  autocrlf = input
  pager = less -F -X
[color "diff"]
  whitespace = red reverse
[pull]
  rebase = true
[rerere]
  enabled = true
[grep]
  lineNumber = 1
[rebase]
  autosquash = 1
